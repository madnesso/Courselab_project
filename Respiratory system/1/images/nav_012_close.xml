		<param type="file" id="n3_img" text="" def="" x="125" y="55" w="100" h="13"/>
		<param type="label" text="Button tooltip:" x="10" y="82" w="110" h="13"/>
		<param type="edit" id="alt_enabled" text="no" def="Close module" x="125" y="81" w="125" h="13"/>
		<param type="label" text="Confirmation text:" x="10" y="98" w="110" h="13"/>
		<param type="edit" id="confirm" text="no" def="Module window will be closed. Are you sure?" x="125" y="97" w="125" h="13"/>
    </page>
</params>
